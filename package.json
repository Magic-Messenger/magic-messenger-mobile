{
  "name": "magic-mobil-expo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:clean": "expo start --clear",
    "expo:clean": "npx expo prebuild --clean",
    "android": "expo run:android",
    "release-aab": "cd android && ./gradlew bundleRelease && cd ..",
    "release-apk": "cd android && ./gradlew assembleRelease  && cd ..",
    "ios": "expo run:ios",
    "ios-device": "npx expo run:ios --device 'iPhone 12'",
    "ios-device-release": "npx expo run:ios --device 'iPhone 12' --configuration Release",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "env:development": "eas env:pull --environment development",
    "env:production": "eas env:pull --environment production",
    "create-page": "node scripts/create-page.js",
    "create-component": "node scripts/create-component.js",
    "generate-api": "orval --config orval.config.ts",
    "generate-api:watch": "orval --config orval.config.ts --watch",
    "build:ios": "eas build -p ios --profile production && eas submit -p ios --profile production",
    "build:android": "eas build -p android --profile production && eas submit -p android --profile production",
    "prepare-hooks": "husky install",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5",
    "@gorhom/portal": "^1.0.14",
    "@logrocket/react-native": "^1.58.1",
    "@microsoft/signalr": "^9.0.6",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@shopify/flash-list": "^2.0.3",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.10.0",
    "dayjs": "^1.11.18",
    "expo": "~53.0.20",
    "expo-application": "~6.1.5",
    "expo-av": "~15.1.7",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.6",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-iap": "^3.1.9",
    "expo-image": "~2.3.0",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.5",
    "expo-localization": "~16.1.5",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.4",
    "expo-screen-capture": "~7.2.0",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-tor": "file:./modules/expo-tor",
    "expo-video": "~2.2.2",
    "expo-web-browser": "~14.2.0",
    "i18next": "^25.2.1",
    "lodash": "^4.17.21",
    "lottie-react-native": "7.2.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.60.0",
    "react-i18next": "^15.5.3",
    "react-native": "0.79.6",
    "react-native-capture-protection": "^2.3.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-size-matters": "^0.4.2",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@types/lodash": "^4.17.20",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "orval": "^7.10.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
