{
  "name": "magic-mobil-expo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:clean": "expo start --clear",
    "expo:clean": "npx expo prebuild --clean",
    "android": "expo run:android",
    "release-aab": "cd android && ./gradlew bundleRelease && cd ..",
    "release-apk": "cd android && ./gradlew assembleRelease  && cd ..",
    "ios": "expo run:ios",
    "ios-device": "npx expo run:ios --device 'iPhone 12'",
    "ios-device-release": "npx expo run:ios --device 'iPhone 12' --configuration Release",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "env:development": "eas env:pull --environment development",
    "env:production": "eas env:pull --environment production",
    "create-page": "node scripts/create-page.js",
    "create-component": "node scripts/create-component.js",
    "generate-api": "orval --config orval.config.ts",
    "generate-api:watch": "orval --config orval.config.ts --watch",
    "build:ios": "eas build -p ios --profile production && eas submit -p ios --profile production",
    "build:android": "eas build -p android --profile production && eas submit -p android --profile production",
    "prepare-hooks": "husky install",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5",
    "@gorhom/portal": "^1.0.14",
    "@logrocket/react-native": "^1.58.1",
    "@microsoft/signalr": "^9.0.6",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@shopify/flash-list": "2.2.0",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.10.0",
    "dayjs": "^1.11.18",
    "expo": "^54.0.0",
    "expo-application": "~7.0.7",
    "expo-av": "~16.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-camera": "~17.0.9",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.18",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-iap": "^3.1.9",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-localization": "~17.0.7",
    "expo-notifications": "~0.32.13",
    "expo-router": "~6.0.15",
    "expo-screen-capture": "~8.0.8",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-tor": "file:./modules/expo-tor",
    "expo-video": "~3.0.14",
    "expo-web-browser": "~15.0.9",
    "i18next": "^25.2.1",
    "lodash": "^4.17.21",
    "lottie-react-native": "~7.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "react-i18next": "^15.5.3",
    "react-native": "0.81.5",
    "react-native-capture-protection": "^2.3.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-image-viewing": "^0.2.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@types/lodash": "^4.17.20",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "orval": "^7.10.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
